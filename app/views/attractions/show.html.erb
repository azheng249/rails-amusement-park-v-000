<div class="row">
  <h3><%= @attraction.name %></h3><hr>

  <div class="col-xs-9">
    <%= render "attraction" %><br>
  </div>

  <div class="col-xs-3">
    <% if current_user.admin %>
      <%= link_to "Edit Attraction", edit_attraction_path %><br>
    <% else %>
      <%= render "rides/ride_form"%><br>
    <% end %>

    <br>
    <%= link_to "Back", attractions_path %>
  </div>

</div>