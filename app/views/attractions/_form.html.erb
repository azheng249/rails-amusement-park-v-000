<%= form_for @attraction do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><hr>

  <%= f.label :tickets %>
  <%= f.number_field :tickets %><hr>

  <%= f.label :nausea_rating %>
  <%= f.number_field :nausea_rating %><hr>

  <%= f.label :happiness_rating %>
  <%= f.number_field :happiness_rating %><hr>

  <%= f.label :min_height %>
  <%= f.number_field :min_height %><hr>

  <%= f.submit %>  
<% end %>